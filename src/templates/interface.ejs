
import { RequestGenericInterface } from 'fastify';

// Interfaces for <%= model %> requests

export interface I<%= model %>CreateRequest extends RequestGenericInterface {
    Body: <%= model %>CreateBody;
}

export interface I<%= model %>UpdateRequest extends RequestGenericInterface {
    Body: <%= model %>UpdateBody;
    Params: <%= model %>Params;
}

export interface I<%= model %>DeleteRequest extends RequestGenericInterface {
    Params: <%= model %>Params;
}

// Body and Params interfaces

export interface <%= model %>CreateBody {
    [key: string]: any; // This is a generic type. Modify as needed.
}

export interface <%= model %>UpdateBody {
    [key: string]: any; // This is a generic type. Modify as needed.
}

export interface <%= model %>Params {
    id: string;
}